 column=(type=datetime updatewhereclause=yes name=sales_order_order_date dbname="sales_order.order_date" )
 retrieve="  SELECT customer.fname,   
         customer.lname,   
         customer.company_name,   
         customer.address,   
         customer.city,   
         customer.state,   
         customer.zip,   
         employee.emp_fname,   
         employee.emp_lname,   
         sales_order.id,   
         sales_order.order_date  
    FROM customer,   
         employee,   
         sales_order  
   WHERE ( employee.emp_id = sales_order.sales_rep ) and  
         ( customer.id = sales_order.cust_id ) and  
         ( ( customer.id  not in (SELECT cust_id FROM sales_order WHERE (order_date >= :last_order_date) ) ) AND  
         ( customer.id + convert(datetime, sales_order.order_date, 0 ) in (SELECT cust_id +  convert(datetime,Max(order_date),0) FROM sales_order GROUP BY cust_id) ) )
ORDER BY employee.emp_lname ASC,   
         employee.emp_fname ASC,   
         customer.company_name ASC   
" arguments=(("last_order_date", datetime)) )
